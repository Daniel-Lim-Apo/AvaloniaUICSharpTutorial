<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:vm="using:AppAvaloniaMVVMPerson.ViewModels"
             x:Class="AppAvaloniaMVVMPerson.Views.DetailView"
			 x:DataType="vm:DetailViewModel"
			 >
	<StackPanel>
		<TextBox Text="{Binding Person.Name}" />
		<TextBox Text="{Binding Person.DocId}" />
		<TextBox Text="{Binding Person.Gender}" />
		<TextBox Text="{Binding Person.Age}" />

	</StackPanel>
</UserControl>

